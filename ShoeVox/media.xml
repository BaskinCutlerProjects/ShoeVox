<?xml version="1.0" encoding="utf-8" ?>
<programs>
  <program>
    <name>Windows Media Center</name>
    <process>ehshell</process>
    <commands>
      <command>
        <name>play</name>
        <keys>^p</keys>
      </command>
      <command>
        <name>pause</name>
        <keys>^p</keys>
      </command>
      <command>
        <name>stop</name>
        <keys>^+(s)</keys>
      </command>
      <command>
        <name>mute</name>
        <keys>{f8}</keys>
      </command>
      <command>
        <name>volume up</name>
        <keys>{f10}</keys>
      </command>
      <command>
        <name>volume down</name>
        <keys>{f9}</keys>
      </command>
      <command>
        <name>previous</name>
        <!-- ^b just restarts the song, needs to be sent twice -->
        <keys>^b^b</keys>
      </command>
      <command>
        <name>next</name>
        <keys>^f</keys>
      </command>
    </commands>
  </program>
